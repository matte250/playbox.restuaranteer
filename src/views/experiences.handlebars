<ul class="collection with-header">
    <li class="collection-header" style="
        display: flex;
        justify-content: space-between;
        flex-direction: row;
        align-items: center;
        ">
        <h3>Experiences</h3>
        <a href="addexperience" class="btn-floating btn-large waves-effect waves-light red">
            <i class="material-icons">add</i>
        </a>
    </li>
    {{#each experiences}}
    <li class="collection-item">
        <div style="display: inline-block;">
            <h5 style="display: inline-block;">
                {{this.at.name}}<br/>
                {{this.when}}
            </h5>
         </div> 
        <a href="editplaces/{{this.id}}" class="secondary-content">Edit</a>
    </li>
    {{/each}}
</ul>